<div>
    {{>contactsList}}
    <div>
        <form action="/add" method="post">
            <label for="fullName">ФИО</label><input id="fullName"
                required
                name="fullName"
                type="text" />
            <label for="phoneNumber">Номер телефона</label><input id="phoneNumber"
                required
                name="phoneNumber"
                type="text" />
            <div class="buttons-section">
